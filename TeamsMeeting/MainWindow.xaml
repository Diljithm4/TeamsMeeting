<Window x:Class="TeamsMeeting.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamsMeeting"
        mc:Ignorable="d"
        Title="TeamsMeeting" Height="450" Width="800">
    <Grid>
        <Button Content="Join Meeting" HorizontalAlignment="Left" Margin="590,0,0,0" VerticalAlignment="Center" Height="26" Width="152" Click="Button_Join"/>
        <Button Content="Clear" HorizontalAlignment="Left" Margin="472,0,0,0" VerticalAlignment="Center" Width="100" Height="26" Click="Button_Clear" AutomationProperties.Name="BtnClear" RenderTransformOrigin="0.27,-1.845"/>
        <TextBox x:Name="TextBox" HorizontalAlignment="Left" Height="98" Margin="73,0,0,0" TextWrapping="Wrap" Text=""
                 VerticalAlignment="Center" Width="369" TextChanged="TextBox_TextChanged"/>

    </Grid>
</Window>
